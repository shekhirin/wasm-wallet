{
  "name": "Wasm Wallet",
  "description": "WebAssembly Ethereum wallet",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "notifications",
    "storage"
  ],
  "background": "bg.html"
}
